<% provide(:title, 'All users') %>
  <h1>All users</h1>
  <ul class="users">
    <%= paginate @users %>

    <%= render @users %>

    <div class="text-center">
    <%= paginate @users, :theme => 'twitter-bootstrap-3' %><br>
    <%= paginate @users, :theme => 'twitter-bootstrap-3', :pagination_class => "pagination-sm" %><br>
    <%= paginate @users, :theme => 'twitter-bootstrap-3', :pagination_class => "pagination-small pagination-centered" %><br>
    </div>
  </ul>
